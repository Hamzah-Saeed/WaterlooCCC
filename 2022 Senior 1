import java.util.*;

public class Main {

    public static void main(String[] args) {


        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int count = 0;

        if (num%5==0)
            count++;
        for(int i = 0; i<num; i+=5){
            if((num-i)%4==0)
                count++;
        }

        System.out.println(count);


    }
}
